{
  "software": [
    {
      "name": "Docker",
      "version": "19.03",
      "installationSteps": {
        "Ubuntu": [
          {
            "type": "deploy",
            "value": "{{SYSTEM.HOME}}/Core/Utils:{{SERVER.SERVER_HOME}}/Utils"
          },
          {
            "type": "skipCondition",
            "value": "docker --version"
          },
          {
            "type": "command",
            "value": "apt update -y"
          },
          {
            "type": "command",
            "value": "apt upgrade -y"
          },
          {
            "type": "command",
            "value": "apt autoremove -y"
          },
          {
            "type": "packages",
            "value": "selinux-utils, apt-transport-https, ca-certificates, curl, gnupg-agent, software-properties-common, lsb-core"
          },
          {
            "type": "command",
            "value": "curl -fsSL https://download.docker.com/linux/ubuntu/gpg | apt-key add -"
          },
          {
            "type": "command",
            "value": "apt-key fingerprint 0EBFCD88"
          },
          {
            "type": "command",
            "value": "add-apt-repository \"deb https://download.docker.com/linux/ubuntu $(lsb_release -cs) stable\""
          },
          {
            "type": "command",
            "value": "apt-get update"
          },
          {
            "type": "packages",
            "value": "docker-ce=5:20.10.1~3-0~ubuntu-focal, docker-ce-cli=5:20.10.1~3-0~ubuntu-focal, containerd.io, telnet"
          },
          {
            "type": "command",
            "value": "docker run --rm hello-world"
          },
          {
            "type": "reboot",
            "value": "480"
          },
          {
            "type": "command",
            "value": "docker run --rm hello-world"
          }
        ]
      }
    }
  ]
}